{
  "swagger": "2.0",
  "info": {
    "version": "1.0.0",
    "title": "My SwaggerShop Project",
    "description": "My SwaggerShop Project Application API",
    "license": {
      "name": "MIT",
      "url": "https://opensource.org/licenses/MIT"
    }
  },
  "host": "localhost:3000",
  "basePath": "/",
  "paths": {
    "/product": {
      "get": {
        "tags": ["Product"],
        "summary": "Get all products in system",
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "definitions": {
    "Product": {
      "required": ["_id", "libelle", "price", "tags"],
      "properties": {
        "_id": {
          "type": "string",
          "uniqueItems": true
        },
        "libelle": {
          "type": "string"
        },
        "price": {
          "type": "double"
        },
        "tags": {
          "type": "array"
        }
      }
    }
  }
}
