     "definitions": {
    "Product": {
    "required": [
    "_id",
    "libelle",
    "price",
    "tags"
    ],
    "properties": {
    "_id": {
    "type": "string",
    "uniqueItems": true
    },
    "libelle": {
    "type": "string"
    },
    "price": {
    "type": "double"
    },
    "tags": {
      "type": "array"
    }
    }
    }
  }
